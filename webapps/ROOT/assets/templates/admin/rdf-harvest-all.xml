<?xml version="1.0" encoding="utf-8"?>
<kiln:root xmlns:xi="http://www.w3.org/2001/XInclude"
           xmlns:kiln="http://www.kcl.ac.uk/artshums/depts/ddh/kiln/ns/1.0"
           xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

  <xsl:import href="cocoon://_internal/template/xsl/stylesheets/rdf/harvest-all-to-html.xsl" />

  <xsl:param name="file" />

  <kiln:parent>
    <xi:include href="base.xml" />
  </kiln:parent>

  <kiln:child>
    <kiln:block name="title">
      <kiln:super />
      <xsl:text>RDF harvest results for all documents</xsl:text>
    </kiln:block>

    <kiln:block name="js">
      <kiln:super />
      <script src="{$kiln:assets-path}/scripts/admin.js">&#160;</script>
    </kiln:block>

    <kiln:block name="header">
      <h1>Kiln Admin: RDF harvest results for all documents</h1>
    </kiln:block>

    <kiln:block name="content">
      <xsl:apply-templates select="/aggregation/xincludes" mode="rdf" />
    </kiln:block>
  </kiln:child>

</kiln:root>
