<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.stoa.org/epidoc/schema/latest/tei-epidoc.rng" schematypens="http://relaxng.org/ns/structure/1.0"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>Authority list of divine names and epithets in IOSPE</title>
            </titleStmt>
            <publicationStmt><ab/></publicationStmt>
            <sourceDesc><ab/></sourceDesc>
        </fileDesc>
    </teiHeader>
    <text>
        <body>
            <div type="commentary">
                <listPerson type="divinities">
                    <head xml:lang="en">Divine and mythological figures</head>
                    <head xml:lang="ru">RU: Divine and mythological figures</head>
                    <person xml:id="adam">
                        <persName xml:lang="en">Adam</persName>
                        <persName xml:lang="ru">Адам</persName>
                        <persName xml:lang="grc">Ἀδάμ</persName>
                    </person>
                    <person xml:id="theos">
                        <persName xml:lang="en">God</persName>
                        <persName xml:lang="ru">Бог</persName>
                        <persName xml:lang="grc">Θεός</persName>
                        <persName xml:lang="grc">Κύριος</persName>
                        <persName xml:lang="grc">Σαβαώθ</persName>
                    </person>
                    <person xml:id="maria">
                        <persName xml:lang="en">Virgin Mary</persName>
                        <persName xml:lang="ru">Богородица</persName>
                        <persName xml:lang="grc">Θεοτόκος</persName>
                   </person>
                    <person xml:id="iesous">
                        <persName xml:lang="en">Jesus Christ</persName>
                        <persName xml:lang="ru">Иисус Христос</persName>
                        <persName xml:lang="grc">Ἰησοῦς Χριστός</persName>
                    </person>
                    <person sameAs="#theos">
                        <persName xml:lang="en">Kyrios</persName>
                        <persName xml:lang="ru">Господь</persName>
                        <persName xml:lang="grc">Κύριος</persName>
                    </person>
                    <person sameAs="#theos">
                        <persName xml:lang="en">Sabaoth</persName>
                        <persName xml:lang="ru">Саваоф</persName>
                        <persName xml:lang="grc">Σαβαώθ</persName>
                    </person>
                    <person sameAs="#iesous">
                        <persName xml:lang="en">Christ</persName>
                        <persName xml:lang="ru">Христос</persName>
                        <persName xml:lang="grc">Χριστός</persName>
                    </person>
                </listPerson>
                
                <listPerson type="saints">
                    <head xml:lang="en">Saints</head>
                    <head xml:lang="ru">RU: Saints</head>
                    <person xml:id="stgeorge">
                        <persName xml:lang="en">St George</persName>
                        <persName xml:lang="ru">Св. Георгий</persName>
                        <persName xml:lang="grc">Ἅγιος Γεώργιος</persName>
                    </person>
                    <person xml:id="stDemetrius">
                        <persName xml:lang="en">St Demetrius</persName>
                        <persName xml:lang="ru">Св. Димитрий</persName>
                        <persName xml:lang="grc">Ἅγιος Δημήτριος</persName>
                    </person>
                    <person xml:id="sthelen">
                        <persName xml:lang="en">St Helen</persName>
                        <persName xml:lang="ru">Св. Елена</persName>
                        <persName xml:lang="grc">ἁγία ἔνδοξος θεόστεπτος μεγάλη βασιλίσσα καὶ εἰσαπόστολος Ἑλένη</persName>
                    </person>
                    <person xml:id="sttheodore">
                        <persName xml:lang="en">St Theodore</persName>
                        <persName xml:lang="ru">Св. Феодор</persName>
                        <persName xml:lang="grc">Ἅγιος Θεόδωρος</persName>
                    </person>
                    <person xml:id="stjohn">
                        <persName xml:lang="en">St John</persName>
                        <persName xml:lang="ru">Св. Иоанн</persName>
                        <persName xml:lang="grc">Ἅγιος Ἰωάννης</persName>
                    </person>
                    <person xml:id="stjohnbaptist">
                        <persName xml:lang="en">St John the Baptist</persName>
                        <persName xml:lang="ru">Св. Иоанн Предтеча</persName>
                        <persName xml:lang="grc">Ἅγιος Ἰωάννης ὁ Πρόδρομος</persName>
                    </person>
                    <person xml:id="stjohnarchbishop">
                        <persName xml:lang="en">St John of Gothia</persName>
                        <persName xml:lang="ru">Св. Иоанн Готский</persName>
                        <persName xml:lang="grc">ὁ ἐν ἁγ̣ίοι̣ς πατήρ ἡμῶν καὶ ἀρχιεπίσκοπος πόλεως Θεοδώρους καὶ πάσης Γοτθίας Ἰωάννης 
                            ὁ ὁμολογητής</persName>
                    </person>
                    <person xml:id="stjohntheologos">
                        <persName xml:lang="en">St John Theologos</persName>
                        <persName xml:lang="ru">Св. Иоанн Богослов</persName>
                        <persName xml:lang="grc">Ἰωάννης Θεολὀγος</persName>
                    </person>
                    <person xml:id="stcapiton">
                        <persName xml:lang="en">St Capiton</persName>
                        <persName xml:lang="ru">Св. Капитон</persName>
                        <persName xml:lang="grc">Ἅγιος Καπίτων</persName>
                    </person>
                    <person xml:id="stconstantine">
                        <persName xml:lang="en">St Constantine</persName>
                        <persName xml:lang="ru">Св. Константин</persName>
                        <persName xml:lang="grc">ἅγιος ἔνδοξος θεόστεπτος μέγας βασιλεύς καὶ εἰσαπόστολος Κωνσταντῖνος</persName>
                    </person>
                    <person xml:id="stnicolas">
                        <persName xml:lang="en">St Nicholas</persName>
                        <persName xml:lang="ru">Св. Николай</persName>
                        <persName xml:lang="grc">Νικόλαος μιτροπολίτης Μ̣υρῶν</persName>
                    </person>
                    <person xml:id="stpanteleimon">
                        <persName xml:lang="en">St Panteleimon</persName>
                        <persName xml:lang="ru">Св. Пантелеимон</persName>
                        <persName xml:lang="grc">ὁ ἅγιος μεγαλομάρτυς καὶ ἀνάργυρος Παντελεήμων</persName>
                    </person>
                    <person xml:id="stpaul">
                        <persName xml:lang="en">St Paul</persName>
                        <persName xml:lang="ru">Св. Павел</persName>
                        <persName xml:lang="grc">ὁ ἅγιος ἔνδοξος, πανεύφημος καὶ πρωτοκορυφαῖος ἀπόστολος Παῦλος</persName>
                    </person>
                    <person xml:id="stpeter">
                        <persName xml:lang="en">St Peter</persName>
                        <persName xml:lang="ru">Св. Петр</persName>
                        <persName xml:lang="grc">ὁ ἅγιος ἔνδοξος, πανεύφημος καὶ πρωτοκορυφαῖος ἀπόστολος Πέτρος</persName> 
                    </person>
                    <person xml:id="ststephen">
                        <persName xml:lang="en">St Stephen</persName>
                        <persName xml:lang="ru">Св. Стефан</persName>
                        <persName xml:lang="grc">Ἅγιος Στέφανος</persName>
                    </person>
                </listPerson>
            </div>
        </body>
    </text>
</TEI>