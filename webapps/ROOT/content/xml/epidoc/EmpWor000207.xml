                    <ab> 
                        <lb n="1"/><persName type="attested"><name>Vibiae</name> <persName type="patronym"><name><expan><abbr>C</abbr><ex>ai</ex></expan></name> <expan><abbr>f</abbr><ex>iliae</ex></expan></persName> <name>S<supplied reason="lost">ab</supplied><unclear>i</unclear>nae</name></persName>
                        <lb n="2"/><supplied reason="lost">sace</supplied><unclear>r</unclear>doti <persName type="divine"><name>Iu<supplied reason="lost">liae</supplied></name> <name><expan><abbr><supplied reason="lost">Au</supplied><unclear>g</unclear></abbr><ex>ustae</ex></expan></name></persName>
                    </ab>
                </div>
                <div subtype="textpart" type="face" n="back">
                    <ab>
                        <lb n="1"/><persName type="attested"><name>Vi<unclear>b</unclear><supplied reason="lost">iae</supplied></name> <persName type="patronym"><name><expan><abbr>C</abbr><ex>ai</ex></expan></name> <expan><abbr>f</abbr><ex>iliae</ex></expan></persName> <name>Sabin<supplied reason="lost">ae</supplied></name></persName>
                        <lb n="2"/><supplied reason="lost">sacerdoti <persName type="divine"><name>Iulia</name> <name><expan><abbr>Aug</abbr><ex>ustae</ex></expan></name></persName></supplied>
