                    <lb n="3"/><persName type="attested"><name><supplied reason="lost">Sep</supplied>timius</name> <name>Amandus</name></persName>
                    <lb n="4"/><supplied reason="lost"><expan><abbr>v</abbr><ex>ir</ex></expan></supplied> <expan><abbr>p</abbr><ex>erfectissimus</ex></expan> rationalis dev<supplied reason="lost">o</supplied>
                    <lb n="5" break="no"/><supplied reason="lost">t</supplied>us <persName type="divine" key="numenAugusti"><name>numini</name></persName> ma<supplied reason="lost">i</supplied>
                    <lb n="6" break="no"/><supplied reason="lost">e</supplied>statique eorum
