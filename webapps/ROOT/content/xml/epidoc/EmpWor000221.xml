                    <lb n="4"/><persName type="attested"><name>Anteros</name> <persName><name>Lacutula<hi rend="ligature">ni</hi></name> <supplied reason="omitted">servus</supplied></persName></persName>
                    <lb n="5"/><expan><abbr>minist</abbr><ex>ri</ex></expan> <expan><abbr>prim</abbr><ex>i</ex></expan> <persName type="divine" key="fortunaAugusta"><name><expan><abbr>Fortun</abbr><ex>ae</ex></expan></name> <name><expan><abbr>Aug</abbr><ex>ustae</ex></expan></name></persName> <expan><abbr>iuss</abbr><ex>u</ex></expan>
                    <lb n="6"/><persName type="attested"><name><expan><abbr>M</abbr><ex>arci</ex></expan></name> <name>Stai</name> <name>Rufi</name></persName> <persName type="attested"><name><expan><abbr>Cn</abbr><ex>aei</ex></expan></name> <name>Melissaei</name></persName> <expan><abbr>d</abbr><ex>uum</ex><abbr>v</abbr><ex>irorum</ex></expan> <expan><abbr>i</abbr><ex>ure</ex></expan> <expan><abbr>d</abbr><ex>icundo</ex></expan>
                    <lb n="7"/><persName type="attested"><name><expan><abbr>P</abbr><ex>ublio</ex></expan></name> <name>Silio</name></persName> <persName type="attested"><name><expan><abbr>L</abbr><ex>ucio</ex></expan></name> <name>Volusio</name> <name><expan><abbr>Saturn</abbr><ex>ino</ex></expan></name></persName> <expan><abbr>co</abbr><ex>n</ex><abbr>s</abbr><ex>ulibus</ex></expan>
                </ab>
