            <div type="edition" xml:space="preserve" xml:lang="la" resp="#AA">
                <ab> 
                    <lb n="1"/><persName type="attested"><name>Agathemerus</name> <persName type="attested"><name>V<unclear>e</unclear><supplied reason="undefined" evidence="previouseditor">tti</supplied></name> <supplied reason="omitted">servus</supplied></persName></persName>
                    <lb n="2"/><persName type="attested"><name>Suavis</name> <persName><name>Caesiae</name> <name>Pri<hi rend="ligature">ma</hi><unclear>e</unclear></name> <supplied reason="omitted">servus</supplied></persName></persName>
