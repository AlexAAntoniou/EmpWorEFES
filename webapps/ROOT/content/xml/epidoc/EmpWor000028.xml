                <ab> <!-- CIL -->
                   <lb n="1"/><persName type="divinity"><name>Numini</name></persName> domus <persName type="attested"><name><expan><abbr>A<supplied reason="lost">ug</supplied></abbr><ex>ustae</ex></expan></name></persName>
                   <lb n="2"/><persName type="attested"><name>Blastus</name> <name>Eutact<supplied reason="lost" cert="low">ianus</supplied></name></persName> <supplied reason="lost">et</supplied>
