                        <lb n="2"/><name>Petronio</name> <name>Karo</name></persName>
                        <lb n="3"/><expan><abbr>sp</abbr><ex>lendido</ex></expan> <expan><abbr>eq</abbr><ex>uiti</ex></expan> <expan><abbr>R</abbr><ex>omano</ex></expan> <expan><abbr>fl</abbr><ex>amini</ex></expan> <persName type="divine" key="divusAugustus"><name>divi</name> <name><expan><abbr>Aug</abbr><ex>usti</ex></expan></name></persName>
