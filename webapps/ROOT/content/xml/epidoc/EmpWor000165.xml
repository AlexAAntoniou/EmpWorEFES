                    <lb n="9"/><expan><abbr>praet</abbr><ex>orio</ex></expan> <expan><abbr><hi rend="supraline">eemm</hi></abbr><ex>inentissimorum</ex></expan> <expan><abbr><hi rend="supraline">vv</hi></abbr><ex>irorum</ex></expan> et
