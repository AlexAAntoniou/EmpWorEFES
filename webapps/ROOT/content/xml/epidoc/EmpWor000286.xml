            <div type="edition" xml:space="preserve" xml:lang="la" resp="#AA">
                <ab>
                    <lb n="1"/><persName type="attested"><name><expan><abbr>Sex</abbr><ex>to</ex></expan></name> <name>Aulieno</name> <persName type="patronym"><name><expan><abbr>Sex</abbr><ex>ti</ex></expan></name> <expan><abbr>f</abbr><ex>ilio</ex></expan></persName>
